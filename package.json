{
  "name": "cli-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project important-app -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npx-build-plus:copy-assets": "node projects/important-app/copy-wc-polyfill.js && node projects/external/copy-wc-polyfill.js",
    "start:ce": "ng serve -o --project external",
    "build:ce": "npm run bundle:ce && npm run copy:ce",
    "copy:ce": "cpr dist/external/main.js projects/important-app/src/assets/external-dashboard-tile.bundle.js -o",
    "copy:modern": "cpr dist/external/main.modern.js projects/important-app/src/assets/external-dashboard-tile.modern.bundle.js -o",
    "copy:legacy": "cpr dist/external/main.legacy.js projects/important-app/src/assets/external-dashboard-tile.legacy.bundle.js -o",
    "copy:ce:modern": "npm run copy:modern && npm run copy:legacy",
    "bundle:ce": "ng build --prod --project external --output-hashing none --single-bundle"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
    "@angular/common": "^8.0.3",
    "@angular/compiler": "^8.0.3",
    "@angular/core": "^8.0.3",
    "@angular/elements": "^8.0.3",
    "@angular/forms": "^8.0.3",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "^8.0.3",
    "@angular/platform-browser-dynamic": "^8.0.3",
    "@angular/router": "^8.0.2",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@swimlane/ngx-charts": "^10.0.0",
    "@webcomponents/custom-elements": "^1.1.2",
    "@webcomponents/shadydom": "^1.3.1",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "angular-oauth2-oidc": "^4.0.2",
    "bootstrap": "^3.3.7",
    "classlist.js": "^1.1.20150312",
    "copy": "^0.3.2",
    "core-js": "^2.5.4",
    "ngx-build-plus": "^8.1.1",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.1.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.4",
    "@angular-devkit/build-ng-packagr": "^0.800.4",
    "@angular/cli": "~8.0.4",
    "@angular/compiler-cli": "^8.0.3",
    "@angular/language-service": "^8.0.3",
    "@ngrx/schematics": "^6.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cpr": "^3.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~3.4.5"
  }
}
